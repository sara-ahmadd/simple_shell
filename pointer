/**
 * free_list - function that frees a list_t list
 * @head: pointer to the head of the list
 * Return: void
 */

void free_list(node *head)
{
	node *temp;

	while (head != NULL)
	{
		temp = head;
		head = head->next;
		free(temp->data);
		free(temp);
	}
}

/**
 * free_list - function that frees a list_t list
 * @head: pointer to the head of the list
 * Return: void
 */

void free_list(node *head)
{
	node *temp;

	while (head != NULL)
	{
		temp = head;
		head = head->next;
		free(temp->data);
		free(temp);
	}
}
